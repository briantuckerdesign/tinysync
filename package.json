{
    "name": "tinysync",
    "version": "0.7.0",
    "author": "Brian Tucker",
    "private": false,
    "description": "tinysync monorepo - Sync data between Airtable and Webflow",
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "typecheck": "bun tsc --noEmit",
        "clean": "rm -rf packages/*/node_modules node_modules",
        "publish:all": "bun run build && bun run publish:core && bun run publish:cli",
        "publish:core": "cd packages/core && npm publish --access public",
        "publish:cli": "cd packages/cli && npm publish --access public"
    },
    "keywords": [
        "airtable",
        "webflow",
        "sync",
        "automation"
    ],
    "type": "module",
    "license": "ISC",
    "dependencies": {
        "@clack/prompts": "^1.0.0-alpha.8",
        "@ts-stack/markdown": "^1.5.0",
        "airtable": "^0.12.2",
        "ascii-table3": "^1.0.1",
        "figlet": "^1.9.4",
        "picocolors": "^1.1.1",
        "uuid": "^13.0.0",
        "webflow-api": "^3.2.1"
    },
    "devDependencies": {
        "@types/bun": "^1.3.4",
        "@types/figlet": "^1.7.0",
        "prettier": "^3.7.4",
        "typescript": "^5.9.3"
    }
}
