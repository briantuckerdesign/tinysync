{
    "name": "@tinysync/cli",
    "version": "1.0.7",
    "description": "CLI tool for tinysync - sync data between Airtable and Webflow",
    "main": "./dist/index.js",
    "bin": {
        "tinysync": "./dist/index.js"
    },
    "files": [
        "dist/index.js",
        "dist/index.js.map",
        "README.md",
        "LICENSE.txt"
    ],
    "scripts": {
        "start": "bun src/index.ts",
        "start:local": "export NODE_TLS_REJECT_UNAUTHORIZED=0 && bun src/index.ts",
        "build": "bun build.ts",
        "typecheck": "bun tsc --noEmit"
    },
    "keywords": [
        "bun",
        "cli",
        "airtable",
        "webflow",
        "sync",
        "automation"
    ],
    "author": "Brian Tucker",
    "license": "ISC",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "https://github.com/briantuckerdesign/tinysync.git",
        "directory": "packages/cli"
    },
    "dependencies": {
        "@tinysync/core": "^1.0.1",
        "@clack/prompts": "^1.0.0-alpha.8",
        "@ts-stack/markdown": "^1.5.0",
        "ascii-table3": "^1.0.1",
        "picocolors": "^1.1.1"
    },
    "devDependencies": {
        "@types/bun": "^1.3.4",
        "typescript": "^5.9.3"
    }
}
